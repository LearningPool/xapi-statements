{
  "name": "xapi-statements",
  "version": "1.0.0",
  "description": "A service for xAPI statements",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/LearningLocker/xapi-statements.git"
  },
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "start": "node dist/index.js",
    "test": "./node_modules/mocha/bin/mocha $(find dist -name '*.test.js') --timeout 6000",
    "test-s3": "MODELS_REPO=memory STORAGE_REPO=s3 npm run test",
    "test-mongo": "MODELS_REPO=mongo STORAGE_REPO=local npm run test",
    "test-memory": "MODELS_REPO=memory STORAGE_REPO=local npm run test",
    "migrate": "node dist/migrate.js",
    "rollback": "node dist/rollback.js",
    "cover-s3": "nyc --lines 100 --check-coverage --exclude '(dist/testAuthRepo|dist/fetchAuthRepo|dist/mongoModelsRepo|dist/memoryStorageRepo|dist/config.js)' npm run test-s3",
    "cover-memory": "nyc --lines 100 --check-coverage --exclude '(dist/testAuthRepo|dist/fetchAuthRepo|dist/mongoModelsRepo|dist/s3StorageRepo|dist/config.js)' npm run test-memory",
    "cover-mongo": "nyc --lines 100 --check-coverage --exclude '(dist/testAuthRepo|dist/fetchAuthRepo|dist/memoryModelsRepo|dist/s3StorageRepo|dist/config.js)' npm run test-mongo",
    "clean": "rm -rf dist",
    "lint": "./node_modules/tslint/bin/tslint --project ./tsconfig.json -c ./tslint.json",
    "duplication": "./node_modules/jscpd/bin/jscpd"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": "^5.0.0"
  },
  "dependencies": {
    "atob": "^2.0.3",
    "aws-sdk": "^2.97.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "boolean": "^0.1.2",
    "btoa": "^1.1.2",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "file-stream-rotator": "^0.1.0",
    "fs-extra": "^3.0.1",
    "helmet": "^3.5.0",
    "jscommons": "git+https://git@github.com/HT2-Labs/jscommons.git",
    "lodash": "^4.17.4",
    "moment": "^2.18.0",
    "mongodb": "^2.2.26",
    "morgan": "^1.8.1",
    "node-fetch": "^1.6.3",
    "object-hash": "1.1.7",
    "promise-retry": "^1.1.1",
    "rulr": "^3.0.0",
    "source-map-support": "^0.4.11",
    "stream-to-string": "^1.1.0",
    "string-to-stream": "^1.1.0",
    "uuid": "^3.0.1",
    "winston": "^2.3.1",
    "winston-aws-cloudwatch": "^1.5.0",
    "xapi-validation": "git+https://git@github.com/LearningLocker/xapi-validation.git"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.4",
    "@types/body-parser": "^1.16.0",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^2.0.20",
    "@types/express": "^4.0.35",
    "@types/fs-extra": "^3.0.3",
    "@types/helmet": "0.0.34",
    "@types/lodash": "^4.14.52",
    "@types/mocha": "^2.2.39",
    "@types/mongodb": "^2.2.2",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.6",
    "@types/node-fetch": "^1.6.7",
    "@types/object-hash": "^0.5.28",
    "@types/source-map-support": "^0.2.28",
    "@types/uuid": "^2.0.29",
    "@types/winston": "^2.2.0",
    "assert": "^1.4.1",
    "jscpd": "git://github.com/ryansmith94/jscpd.git#prepublish",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.0.0",
    "travis-cov": "^0.2.5",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6"
  }
}
