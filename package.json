{
  "name": "xapi-server",
  "version": "1.0.0",
  "description": "A server for the xAPI",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/LearningLocker/xapi-server.git"
  },
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "start": "node dist/index.js",
    "test": "./node_modules/mocha/bin/mocha dist/**/**/*.test.js dist/**/*.test.js --timeout 6000",
    "migrate": "node dist/migrate.js",
    "rollback": "node dist/rollback.js",
    "cover": "npm run test -- --require blanket --reporter travis-cov",
    "clean": "rm -rf dist",
    "lint": "./node_modules/tslint/bin/tslint --project ./tsconfig.json -c ./tslint.json",
    "duplication": "./node_modules/jscpd/bin/jscpd"
  },
  "config": {
    "blanket": {
      "pattern": [
        "dist"
      ],
      "data-cover-never": [
        "node_modules",
        "dist/**/*.test.js"
      ],
      "data-cover-ecmaVersion": "6",
      "travis-cov": {
        "threshold": 99
      }
    }
  },
  "engines": {
    "node": "^6.0.0",
    "npm": "^3.10.10"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "file-stream-rotator": "^0.1.0",
    "helmet": "^3.5.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.0",
    "morgan": "^1.8.1",
    "node-fetch": "^1.6.3",
    "object-hash": "1.1.7",
    "rulr": "^1.1.0",
    "source-map-support": "^0.4.11",
    "uuid": "^3.0.1",
    "winston": "^2.3.1",
    "xapi-validation": "^1.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.0",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^2.0.20",
    "@types/express": "^4.0.35",
    "@types/helmet": "0.0.34",
    "@types/lodash": "^4.14.52",
    "@types/mocha": "^2.2.39",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.5",
    "@types/node-fetch": "^1.6.7",
    "@types/object-hash": "^0.5.28",
    "@types/source-map-support": "^0.2.28",
    "@types/uuid": "^2.0.29",
    "@types/winston": "^2.2.0",
    "assert": "^1.4.1",
    "blanket": "1.1.9",
    "jscpd": "git://github.com/ryansmith94/jscpd.git#prepublish",
    "mocha": "^3.2.0",
    "travis-cov": "^0.2.5",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6"
  }
}
